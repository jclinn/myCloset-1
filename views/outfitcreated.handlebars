      <!-- using localStorage -->
      <script> 
        var extraimage= localStorage.getItem("extraimage");
        if(extraimage != null) {
          document.getElementById("extraimage").src = extraimage;
        }

        var shoeVal = localStorage.getItem("shoeimage");
        if(shoeVal != null ) {
          document.getElementById("shoeVal").src = shoeVal;
        }

        var pantVal = localStorage.getItem("pantimage");
        if(pantVal != null) {
          document.getElementById("pantVal").src = pantVal;
        } 

        var topVal = localStorage.getItem("topimage");
        if(topVal != null) {
          document.getElementById("topVal").src = topVal;
        }

        localStorage.removeItem("shoeimage");
        localStorage.removeItem("pantimage");
        localStorage.removeItem("topimage");
        localStorage.removeItem("extraimage");
      </script>
